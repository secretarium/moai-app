{
    "name": "@secretarium/moai-expo",
    "version": "0.0.19",
    "private": true,
    "description": "Expo mobile and web application",
    "main": "./src/AppEntry.tsx",
    "scripts": {
        "start": "expo start",
        "web": "yarn web:start",
        "android": "yarn android:start",
        "android:start": "expo start --android",
        "android:build": "expo build:android",
        "ios": "yarn os:start",
        "ios:start": "expo start --ios",
        "ios:build": "npx expo-optimize && expo build:ios",
        "test": "jest",
        "test:mutation": "stryker run stryker.config.js",
        "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
        "lint:fix": "yarn run lint --fix",
        "storybook": "start-storybook -p 6006",
        "licenses": "license-checker --production --json > ./public/licenses.json",
        "licenses-mobile": "npm-license-crawler --production --json ./public/licenses-mobile.json",
        "disclaimer": "node disclaimerGenerator.js",
        "postinstall": "yarn run licenses && yarn run disclaimer && yarn run licenses-mobile"
    },
    "dependencies": {
        "@expo/match-media": "0.0.0-beta.2",
